# Vite env variables for backend integration readiness
# Copy to .env.local (gitignored) and set values for your environment.

# Feature flag: when false, the site behaves exactly as a static frontend
# and no network calls are attempted. When true, the UI will try to call
# the backend API specified by VITE_API_BASE_URL.
VITE_BACKEND_ENABLED=false

# Base URL of your backend API (no trailing slash), e.g.:
# http://localhost:3000
# https://api.example.com
VITE_API_BASE_URL=
